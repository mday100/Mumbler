<h1>Edit User</h1>
<%= form_for(@user) do |f| %>
	<%= f.label :username %>
	<%= f.text_field :username %><br />
	<%= f.label :location %>
	<%= f.text_field :location %><br />
	<%= f.label :bio %>
	<%= f.text_area :bio %><br />
	<%= f.label :password %>
	<%= f.password_field :password %><br >
	<%= f.submit :update %>
<% end %>